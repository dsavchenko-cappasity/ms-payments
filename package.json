{
  "name": "ms-payments",
  "version": "0.0.6",
  "description": "Core of the microservice for handling payments",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "if [[ $NODE_ENV != \"production\" ]]; then ./node_modules/.bin/babel -d ./lib --optional es7.objectRestSpread,es7.classProperties,es7.decorators ./src; fi",
    "prepublish": "npm run compile",
    "test": "BLUEBIRD_DEBUG=1 ./node_modules/.bin/mocha --require ./test/babelhook.js --bail -R spec",
    "test-local": "NODE_ENV=development ./node_modules/.bin/mocha --require ./test/babelhook.js",
    "start": "node ./bin/mservice.js | ./node_modules/.bin/bunyan -o short",
    "generate-docs": "jsdoc ./src -d ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/ms-payments.git"
  },
  "author": "Dmitry Gorbunov <me@tewi.tk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-payments/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-payments#readme",
  "dependencies": {
    "bluebird": "^3.0.5",
    "bunyan": "^1.5.1",
    "bytes": "^2.1.0",
    "common-errors": "^0.5.3",
    "continuation-local-storage": "^3.1.4",
    "ioredis": "^1.10.0",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "ms-amqp-conf": "^0.2.0",
    "ms-amqp-transport": "^0.4.0",
    "ms-validation": "^0.5.6",
    "mservice": "^0.13.1",
    "node-uuid": "^1.4.6",
    "paypal-rest-sdk": "^1.6.4",
    "request-promise": "^1.0.2",
    "urlsafe-base64": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.33",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.4",
    "chai": "^3.4.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-mocha": "^1.1.0",
    "json": "^9.0.3",
    "mocha": "^2.3.3",
    "nock": "^3.3.2",
    "server-destroy": "^1.0.1",
    "sinon": "^1.17.2"
  }
}
